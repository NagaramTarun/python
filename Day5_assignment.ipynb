    "#1\n",
    "n = int(input(\"Enter a positive number: \"))\n",
    "s=0\n",
    "a=0\n",
    "for i in range(1,n+1):\n",
    "    print(i)\n",
    "while a<=n:\n",
    "    s += a\n",
    "    a=a+1\n",
    "print(s)"
